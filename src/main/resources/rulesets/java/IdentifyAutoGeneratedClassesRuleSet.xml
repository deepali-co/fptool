<?xml version="1.0"?>
<ruleset name="IdentifyAutoGeneratedClassesRuleSet"
    xmlns="http://pmd.sourceforge.net/ruleset/2.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://pmd.sourceforge.net/ruleset/2.0.0 http://pmd.sourceforge.net/ruleset_2_0_0.xsd">
    <rule name="IdentifyAutoGeneratedClassesRule"
          message="Identify classes that are auto-generated"
          class="com.myapps.fptool.pmd.rules.IdentifyAutoGeneratedClassesRule">
      <description>
Identify classes that are auto-generated
      </description>
      <priority>3</priority>
      <example>
<![CDATA[
/**
 * @generated
 * @author XXX
 * Auto generated by XXX
 */
public class AutoGenerateClass {

}
]]>
      </example>
    </rule>
</ruleset>
